{
  "name": "claude-flow-swarm-visualization",
  "version": "1.0.0",
  "description": "Real-time WebSocket visualization for Claude Flow Swarm Intelligence with Neo4j-like graph schema",
  "main": "swarm-vis-server.js",
  "scripts": {
    "start": "node swarm-vis-server.js",
    "dev": "nodemon swarm-vis-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:tdd": "jest --watch --verbose tests/unit",
    "test:bdd": "jest --verbose tests/integration",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docker:build": "docker build -t claude-flow-swarm-vis .",
    "docker:run": "docker run -p 8080:8080 claude-flow-swarm-vis",
    "docs": "jsdoc -d docs swarm-vis-server.js public/visualization.js",
    "validate": "npm run lint && npm run test:coverage"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "jsdoc": "^4.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "author": {
    "name": "Michael O'Boyle",
    "email": "michael@oboyle.co",
    "url": "https://oboyle.co"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaeloboyle/claude-flow-swarm-visualization"
  },
  "bugs": {
    "url": "https://github.com/michaeloboyle/claude-flow-swarm-visualization/issues"
  },
  "homepage": "https://github.com/michaeloboyle/claude-flow-swarm-visualization#readme",
  "keywords": [
    "claude-flow",
    "swarm-intelligence",
    "visualization",
    "websocket",
    "real-time",
    "graph-database",
    "neo4j-like",
    "vis.js",
    "tdd",
    "bdd",
    "agent-coordination",
    "ai-visualization"
  ]
}